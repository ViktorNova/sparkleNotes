    <asset entry="true" type="Qnx/Elf" path="${BUILD_DIR}/sparkleNotes">sparkleNotes</asset>
    <asset path="/home/khertan/Projects/libgit2/libgit2.so.21">lib/libgit2.so.21</asset>
    <asset path="/home/khertan/Projects/libssh2/libssh2.so">lib/libssh2.so</asset>
    <asset path="/home/khertan/Projects/sparkleNotes/ownnotes.png">ownnotes.png</asset>
