    <asset path="/home/khertan/Projects/libssh2/libssh2.so" type="Qnx/Elf">lib/libssh2.so</asset>
    <env var="LD_LIBRARY_PATH" value="/accounts/devuser/qt/lib:$LD_LIBRARY_PATH:app/native/lib"/>
